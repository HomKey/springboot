<linked-list>
<#list thList as object>
    <DeviceDefine>
        <deviceId>${object.deviceId}</deviceId>
        <iSensorDefines>
            <SensorDefine>
                <name>TH.Temperature</name>
                <registerIndex>0</registerIndex>
                <scale>2</scale>
                <isMean>false</isMean>
                <commandName>th_r2_d${object.index}_AI</commandName>
                <divider>100</divider>
            </SensorDefine>
            <SensorDefine>
                <name>TH.Humidity</name>
                <registerIndex>1</registerIndex>
                <scale>2</scale>
                <isMean>false</isMean>
                <commandName>th_r2_d${object.index}_AI</commandName>
                <divider>100</divider>
            </SensorDefine>
        </iSensorDefines>
    </DeviceDefine>
</#list>
</linked-list>